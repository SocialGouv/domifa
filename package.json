{
  "name": "domifa",
  "private": true,
  "version": "2.93.1",
  "scripts": {
    "build": "lerna run build --stream",
    "dev": "lerna run dev --parallel --stream",
    "e2e": "yarn --cwd optional/e2e/runners/puppeteer test",
    "lint": "lerna run lint --parallel --stream",
    "start": "lerna run start --parallel --stream",
    "test": "lerna run test --parallel --stream",
    "seal-secrets": "sre-secrets --from=./.secrets.yaml --to=./.k8s"
  },
  "devDependencies": {
    "@socialgouv/sre-secrets": "^1.4.0",
    "lerna": "^5.0.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/bootstrap",
      "**/@angular/*",
      "**/@angular/*/**",
      "**/@angular/compiler-cli/*",
      "**/@angular/compiler-cli/**",
      "**/@ngrx/*",
      "**/@ngrx/*/**",
      "**/@nrwl/*",
      "**/@nrwl/*/**",
      "**/@socialgouv/*/**",
      "**/eslint*/*/**",
      "**/@angular-builders/*",
      "**/@angular-builders/*/**",
      "**/@angular-devkit/*",
      "**/@angular-devkit/*/**",
      "**/@angular-eslint/*",
      "**/@angular-eslint/*/**",
      "**/intl-tel-input",
      "**/intl-tel-input/*",
      "**/intl-tel-input/*/**",
      "**/ngx-bootstrap",
      "**/ngx-intl-tel-input",
      "**/ngx-intl-tel-input/*",
      "**/ngx-intl-tel-input/*/**"
    ]
  },
  "packageManager": "yarn@1.22.18"
}
