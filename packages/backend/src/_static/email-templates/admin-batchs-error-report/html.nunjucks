{% extends "./base.nunjucks" %}
{% block head_custom %}
     <link rel="stylesheet" href="./admin-batchs-error-report/html.css">
{% endblock %}

{% block content %}

     <div class="content text-left leading-normal">
          <div class="my-2">
               <b>{{errorsCount}} erreurs</b> ont été rencontrées lors des traitements sur l'environnement <b>"{{envId}}"</b>
          </div>

          <div class="my-2"><br/>Dernier message d'erreur: 
        <br/>> <i>
                    <b>{{lastErrorMessage}}</b>
               </i>
          </div>

          <div class="my-2"<br/>Date de la dernière erreur: <b>{{lastErrorDate}}</b>
          </div>

          <div class="my-2"><br/>Process en erreur: <b>{{processIds}}</b>
          </div>
     </div>

{% endblock %}