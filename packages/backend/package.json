{
  "name": "@domifa/backend",
  "private": true,
  "version": "0.18.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cp -r src/excel/_templates dist/excel && cp -r src/_static dist/",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:dev:generate": "npx ts-node -r tsconfig-paths/register --transpile-only ./node_modules/.bin/typeorm migration:generate -n CreateDatabase",
    "db:dev:migrate-up": "npx ts-node -r tsconfig-paths/register --transpile-only src/run-migrate-up.ts",
    "db:dev:migrate-redo": "npx ts-node -r tsconfig-paths/register --transpile-only src/run-migrate-up-redo.ts",
    "db:dev:migrate-down:last": "npx ts-node -r tsconfig-paths/register --transpile-only src/run-migrate-down-last.ts",
    "db:test:migrate-up": "NODE_ENV=tests-local npx ts-node -r tsconfig-paths/register --transpile-only src/run-migrate-up.ts",
    "db:test:migrate-redo": "NODE_ENV=tests-local npx ts-node -r tsconfig-paths/register --transpile-only src/run-migrate-up-redo.ts",
    "db:test:migrate-down:last": "NODE_ENV=tests-local npx ts-node -r tsconfig-paths/register --transpile-only src/run-migrate-down-last.ts",
    "db:dev:data-anonymize": "npx ts-node -r tsconfig-paths/register --transpile-only src/run-data-anonymization.ts",
    "db:prod:data-anonymize": "node dist/run-data-anonymization.js",
    "db:prod:migrate-down:last": "node dist/run-migrate-down-last.js",
    "db:prod:migrate-up": "node dist/run-migrate-up.js",
    "db:migration:create": "npx ts-node -r tsconfig-paths/register --transpile-only ./node_modules/.bin/typeorm migration:create -n manual-migration",
    "db:migration:generate": "npx ts-node -r tsconfig-paths/register  --transpile-only ./node_modules/.bin/typeorm migration:generate -n auto-migration",
    "start": "npx ts-node-dev -r tsconfig-paths/register --transpile-only src/run-app.ts",
    "start:check": "npx ts-node-dev -r tsconfig-paths/register src/run-app.ts",
    "start:dev": "nodemon",
    "start:tests-local": "NODE_ENV=tests-local nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/run-app.js",
    "start:tests-ci": "npx ts-node -r tsconfig-paths/register src/run-app.ts",
    "lint": "tslint -p tsconfig.json -t stylish",
    "test": "rimraf dist && NODE_ENV=tests-local jest",
    "test:ci": "NODE_ENV=tests-ci jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/testing": "^7.4.4",
    "@sentry/node": "6.3.5",
    "@types/compression": "^1.7.0",
    "@types/cron": "^1.7.2",
    "@types/express": "4.17.11",
    "@types/multer": "^1.4.5",
    "@types/nodemailer": "^6.4.1",
    "@types/passport-jwt": "^3.0.5",
    "@types/pizzip": "^3.0.1",
    "@types/rimraf": "^3.0.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "braces": "^3.0.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "date-fns": "^2.17.0",
    "date-fns-tz": "^1.1.4",
    "docxtemplater": "^3.19.10",
    "dotenv": "^8.2.0",
    "email-templates": "^8.0.4",
    "exceljs": "^4.2.1",
    "faker": "^5.1.0",
    "file-type": "^16.1.0",
    "fs-extra": "^9.1.0",
    "ical-generator": "^1.15.4",
    "ics": "^2.24.0",
    "multer": "^1.4.2",
    "node-pdftk": "^2.1.3",
    "nodemailer": "^6.4.18",
    "nodemailer-smtp-transport": "^2.7.4",
    "nunjucks": "^3.2.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.2",
    "pizzip": "^3.0.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "xlsx": "^0.16.9",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.7",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/testing": "^7.4.4",
    "@socialgouv/tslint-config-recommended": "^0.35.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^1.7.2",
    "@types/dotenv": "^6.1.1",
    "@types/email-templates": "^8.0.3",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/rimraf": "^3.0.0",
    "@types/supertest": "^2.0.11",
    "check-disk-space": "^2.1.0",
    "commander": "^7.2.0",
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "mammoth": "^1.4.16",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}